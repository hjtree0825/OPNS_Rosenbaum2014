make_data <- function(problem.param){
  problem.param %>% {
    list(
      assembly = 
        tibble(
          x = runif(.$assembly.count),
          y = runif(.$assembly.count)
        ),
      competetor = 
        tibble(
          x = runif(.$competetor.count),
          y = runif(.$competetor.count)
        ),
      beta = 
        tibble(
          distance = .$distance,
          union = .$union
        ),
      union.fn = function(x, y) (x-1/2)^2+(y/4)^2
    )
  }
}